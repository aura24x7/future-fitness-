{
  "cli": {
    "version": "15.0.15",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk",
        "env": {
          "EXPO_DEBUG": "true"
        },
        "withoutCredentials": false
      },
      "ios": {
        "buildConfiguration": "Debug"
      },
      "cache": {
        "disabled": true
      },
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      },
      "extends": "base"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "cache": {
        "disabled": true
      },
      "extends": "base"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "cache": {
        "disabled": false
      },
      "extends": "base"
    },
    "base": {
      "hooks": {
        "postInstall": "npx expo prebuild"
      },
      "android": {
        "image": "ubuntu-22.04-jdk-17-ndk-r21e"
      },
      "ios": {
        "image": "latest"
      }
    }
  }
}
