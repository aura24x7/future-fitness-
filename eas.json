{
  "cli": {
    "version": "15.0.15",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk",
        "image": "ubuntu-22.04-jdk-17-ndk-r21e",
        "env": {
          "EXPO_DEBUG": "true"
        },
        "withoutCredentials": false
      },
      "ios": {
        "buildConfiguration": "Debug",
        "image": "latest"
      },
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      },
      "prebuildCommand": "node ./eas-hooks/prebuild.js",
      "postinstallCommand": "node ./eas-hooks/postinstall.js"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "ubuntu-22.04-jdk-17-ndk-r21e"
      },
      "ios": {
        "image": "latest"
      },
      "prebuildCommand": "node ./eas-hooks/prebuild.js",
      "postinstallCommand": "node ./eas-hooks/postinstall.js"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "image": "ubuntu-22.04-jdk-17-ndk-r21e"
      },
      "ios": {
        "image": "latest"
      },
      "prebuildCommand": "node ./eas-hooks/prebuild.js",
      "postinstallCommand": "node ./eas-hooks/postinstall.js"
    }
  }
}
