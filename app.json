{
  "expo": {
    "name": "AI Fitness",
    "slug": "ai-fitness",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4c669f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aifit.aifitness",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan food items for nutritional tracking.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save food images."
      }
    },
    "android": {
      "package": "com.aifit.aifitness",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4c669f"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "react-native-vision-camera",
      "expo-camera"
    ],
    "extra": {
      "eas": {
        "projectId": "822972139614"
      }
    },
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/822972139614"
    }
  }
}
